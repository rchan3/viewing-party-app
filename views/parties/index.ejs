<%- include('../partials/header') %>

<main>
    <div class="party-show-page">
            <h2>ALL PARTIES</h2>

            <% parties.forEach((p,idx) => { %>
            <h3> <%= p.name %> @ <%= p.date.toLocaleString('en',{month:'2-digit',year:'numeric', day:'2-digit', hour:'numeric', minute: '2-digit'}) %> </h3>
            <a href="/parties/<%= p._id %>">details</a>
            <% }) %>        
    </div>
</main>

<%- include('../partials/footer') %>
   

